#pragma once // clang-format off
#include "abc.h"

#define __ywlib_wide_(x) L##x
#define __ywlib_wide(x) __ywlib_wide_(x)

#ifndef NDEBUG
#define assert(e) (void(!(e) && (__ywlib_global _wassert(L#e, __ywlib_wide(__FILE__), __LINE__), 0)))
#else
#define assert(e) (void(0))
#endif

extern "C" {
__ywlib_begin;
__ywlib_export void _wassert(const wchar_t*, const wchar_t*, unsigned);
__ywlib_end;
}
