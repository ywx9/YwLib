#define LC_ALL 0
#define LC_COLLATE 1
#define LC_CTYPE 2
#define LC_MONETARY 3
#define LC_NUMERIC 4
#define LC_TIME 5

__ywstd_cfunc_begin_poqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoqpoq

struct lconv {
  char* decimal_point;
  char* thousands_sep;
  char* grouping;
  char* int_curr_symbol;
  char* currency_symbol;
  char* mon_decimal_point;
  char* mon_thousands_sep;
  char* mon_grouping;
  char* positive_sign;
  char* negative_sign;
  char int_frac_digits;
  char frac_digits;
  char p_cs_precedes;
  char p_sep_by_space;
  char n_cs_precedes;
  char n_sep_by_space;
  char p_sign_posn;
  char n_sign_posn;
  wchar_t* _W_decimal_point;
  wchar_t* _W_thousands_sep;
  wchar_t* _W_int_curr_symbol;
  wchar_t* _W_currency_symbol;
  wchar_t* _W_mon_decimal_point;
  wchar_t* _W_mon_thousands_sep;
  wchar_t* _W_positive_sign;
  wchar_t* _W_negative_sign;
};
char* __cdecl setlocale(int cat, const char* locale);
lconv* __cdecl localeconv();

__ywstd_cfunc_end___bodbodbodbodbodbodbodbodbodbodbodbodbodbodbodbodbodbodbodbod

#if not __ywstd_is_imported
__ywstd_export namespace std {
using __ywstd_cfunc lconv;
