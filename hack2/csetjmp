/// \file csetjmp
/// This file hacks `<csetjmp>` but not `<setjmp.h>` because no headers include `<setjmp.h>`.

#pragma once
#include "abc.h"

__ywstd_cfunc_begin

__ywstd_export int __cdecl setjmp(jmp_buf);
[[noreturn]] void __cdecl longjmp(jmp_buf, int) noexcept(false);

__ywstd_cfunc_end

namespace std {
__ywstd_export using _CSTD jmp_buf;
__ywstd_export using _CSTD longjmp;
}
