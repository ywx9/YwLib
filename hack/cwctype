#pragma once
#include "abc.h"

extern "C" {
__ywlib_begin;

using wint_t = unsigned short;
using wctype_t = unsigned short;
using wctrans_t = wchar_t;

int __cdecl iswalnum(wint_t);
int __cdecl iswalpha(wint_t);
int __cdecl iswlower(wint_t);
int __cdecl iswupper(wint_t);
int __cdecl iswdigit(wint_t);
int __cdecl iswxdigit(wint_t);
int __cdecl iswcntrl(wint_t);
int __cdecl iswgraph(wint_t);
int __cdecl iswspace(wint_t);
int __cdecl iswblank(wint_t);
int __cdecl iswprint(wint_t);
int __cdecl iswpunct(wint_t);
int __cdecl iswctype(wint_t, wctype_t);

wctype_t __cdecl wctype(const char*);
wint_t __cdecl towupper(wint_t);
wint_t __cdecl towlower(wint_t);
wint_t __cdecl towctrans(wint_t, wctrans_t);
wctrans_t __cdecl wctrans(const char*);

__ywlib_end;
}
__ywlib_export namespace std {

using wint_t = unsigned short;
using wctype_t = unsigned short;
using wctrans_t = wchar_t;

using __ywlib_global iswalnum;
using __ywlib_global iswalpha;
using __ywlib_global iswlower;
using __ywlib_global iswupper;
using __ywlib_global iswdigit;
using __ywlib_global iswxdigit;
using __ywlib_global iswcntrl;
using __ywlib_global iswgraph;
using __ywlib_global iswspace;
using __ywlib_global iswblank;
using __ywlib_global iswprint;
using __ywlib_global iswpunct;
using __ywlib_global iswctype;
using __ywlib_global towupper;
using __ywlib_global towlower;
using __ywlib_global towctrans;
using __ywlib_global wctype;
using __ywlib_global wctrans;

}
