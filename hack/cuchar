#pragma once // clang-format off
#include "abc.h"

#define __STDC_UTF_16__ 1
#define __STDC_UTF_32__ 1

#ifndef WEOF
#define WEOF (std::wint_t)(0xFFFF)
#endif

extern "C" {
__ywlib_begin;
size_t __cdecl mbrtoc16(char16_t*, const char*, size_t, mbstate_t*);
size_t __cdecl c16rtomb(char*, char16_t, mbstate_t*);
size_t __cdecl mbrtoc32(char32_t*, const char*, size_t, mbstate_t*);
size_t __cdecl c32rtomb(char*, char32_t, mbstate_t*);
__ywlib_end;
}
__ywlib_export namespace std {
using __ywlib_global mbstate_t;
using __ywlib_global c16rtomb;
using __ywlib_global c32rtomb;
using __ywlib_global mbrtoc16;
using __ywlib_global mbrtoc32;
}
